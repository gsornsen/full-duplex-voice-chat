{
  "dashboard": {
    "title": "TTS Pipeline Metrics",
    "uid": "tts-pipeline-v1",
    "tags": ["tts", "synthesis", "performance", "sla"],
    "timezone": "browser",
    "schemaVersion": 38,
    "refresh": "10s",
    "panels": [
      {
        "id": 1,
        "title": "Synthesis Latency (p50/p95/p99)",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(synthesis_latency_seconds_bucket[5m]))",
            "legendFormat": "p50",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(synthesis_latency_seconds_bucket[5m]))",
            "legendFormat": "p95 (SLA: 300ms GPU / 500ms CPU)",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, rate(synthesis_latency_seconds_bucket[5m]))",
            "legendFormat": "p99",
            "refId": "C"
          }
        ],
        "yaxes": [
          {
            "format": "s",
            "label": "Latency"
          }
        ],
        "thresholds": [
          {
            "value": 0.3,
            "colorMode": "critical",
            "op": "gt",
            "fill": true,
            "line": true
          }
        ]
      },
      {
        "id": 2,
        "title": "Worker Utilization",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
        "targets": [
          {
            "expr": "worker_utilization_percent",
            "legendFormat": "Utilization %",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "format": "percent",
            "label": "Utilization",
            "min": 0,
            "max": 100
          }
        ]
      },
      {
        "id": 3,
        "title": "Active Sessions",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 8},
        "targets": [
          {
            "expr": "sessions_active",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "auto",
          "orientation": "auto"
        },
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 50, "color": "yellow"},
                {"value": 100, "color": "red"}
              ]
            }
          }
        }
      },
      {
        "id": 4,
        "title": "Total Synthesis Requests",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 6, "y": 8},
        "targets": [
          {
            "expr": "synthesis_total",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area"
        }
      },
      {
        "id": 5,
        "title": "SLA Violations",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 12, "y": 8},
        "targets": [
          {
            "expr": "sla_violations_total",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area"
        },
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 10, "color": "yellow"},
                {"value": 50, "color": "red"}
              ]
            }
          }
        }
      },
      {
        "id": 6,
        "title": "SLA Compliance %",
        "type": "gauge",
        "gridPos": {"h": 4, "w": 6, "x": 18, "y": 8},
        "targets": [
          {
            "expr": "sla_compliance_percent",
            "refId": "A"
          }
        ],
        "options": {
          "showThresholdLabels": false,
          "showThresholdMarkers": true
        },
        "fieldConfig": {
          "defaults": {
            "min": 0,
            "max": 100,
            "unit": "percent",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 95, "color": "yellow"},
                {"value": 99, "color": "green"}
              ]
            }
          }
        }
      },
      {
        "id": 7,
        "title": "Barge-in Latency (p95)",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 12},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(barge_in_latency_seconds_bucket[5m]))",
            "legendFormat": "p95 Barge-in (SLA: <50ms)",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "format": "s",
            "label": "Latency"
          }
        ],
        "thresholds": [
          {
            "value": 0.05,
            "colorMode": "critical",
            "op": "gt",
            "fill": true,
            "line": true
          }
        ]
      },
      {
        "id": 8,
        "title": "Error Rate",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 12},
        "targets": [
          {
            "expr": "rate(synthesis_errors_total[5m])",
            "legendFormat": "Synthesis Errors/sec",
            "refId": "A"
          },
          {
            "expr": "rate(worker_errors_total[5m])",
            "legendFormat": "Worker Errors/sec",
            "refId": "B"
          }
        ],
        "yaxes": [
          {
            "format": "short",
            "label": "Errors/sec"
          }
        ]
      },
      {
        "id": 9,
        "title": "Session Duration Distribution",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 20},
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(session_duration_seconds_bucket[5m]))",
            "legendFormat": "p50 Session Duration",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(session_duration_seconds_bucket[5m]))",
            "legendFormat": "p95 Session Duration",
            "refId": "B"
          }
        ],
        "yaxes": [
          {
            "format": "s",
            "label": "Duration"
          }
        ]
      },
      {
        "id": 10,
        "title": "Active vs Idle Workers",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 20},
        "targets": [
          {
            "expr": "workers_active",
            "legendFormat": "Active Workers",
            "refId": "A"
          },
          {
            "expr": "workers_idle",
            "legendFormat": "Idle Workers",
            "refId": "B"
          }
        ],
        "yaxes": [
          {
            "format": "short",
            "label": "Worker Count"
          }
        ]
      },
      {
        "id": 11,
        "title": "Synthesis Queue Depth",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 28},
        "targets": [
          {
            "expr": "synthesis_queue_depth",
            "legendFormat": "Queue Depth",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "format": "short",
            "label": "Queue Size"
          }
        ]
      },
      {
        "id": 12,
        "title": "Messages per Session",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 28},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(session_messages_bucket[5m]))",
            "legendFormat": "p95 Messages",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "format": "short",
            "label": "Message Count"
          }
        ]
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h"]
    }
  }
}
